package problem;

import java.math.BigInteger;

import primenumber.Primes;
import specialnumbers.Figurate;

import extras.BigSums;

public class Problem78 {
	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		BigSums bs = new BigSums(10);
		Primes p = new Primes(1000000);
		for (int i = 1; i < 10; i++) {
			// if (bs.allSumsUnder(i, i).mod(BigInteger.valueOf(1000)).equals(BigInteger.ZERO)) {
			System.out.println(i + ": " + bs.allSumsUnder2(i) + ", " + p.factorLinkList(i - 1) + ", " + p.factorLinkList(i) + ", " + p.factorLinkList(i + 1));
			// System.out.println(Figurate.Pen(i));
			// }
		}
	}
}
