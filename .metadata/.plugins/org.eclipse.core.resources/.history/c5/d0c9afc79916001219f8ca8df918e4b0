package problem;

import java.math.BigInteger;

import extras.BigSums;

public class Problem78 {
	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		BigSums bs = new BigSums(5000);
		for (int i = 1; i < 5000; i++) {
			if (bs.allSumsUnder(i, i).mod(BigInteger.valueOf(10000)).equals(BigInteger.ZERO)) {
				System.out.println(i + ": " + bs.allSumsUnder(i, i));
			}
		}
	}
}
