package problem;

import java.math.BigInteger;

import primenumber.Primes;

import extras.BigSums;

public class Problem78 {
	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		BigSums bs = new BigSums(5000);
		Primes p = new Primes(1000000);
		for (int i = 2499; i < 5000; i += 1000) {
			// if (bs.allSumsUnder(i, i).mod(BigInteger.valueOf(1000)).equals(BigInteger.ZERO)) {
			System.out.println(i + ": " + bs.allSumsUnder(i, i) + ", " + p.factorLinkList(i - 1) + ", " + p.factorLinkList(i) + ", " + p.factorLinkList(i + 1));
			// }
		}
	}
}
