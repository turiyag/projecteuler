package problem;

import java.math.BigDecimal;
import java.math.BigInteger;

import specialnumbers.NumHelp;

public class Problem79 {
	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		BigInteger bTotalSum = BigInteger.ZERO;
		BigInteger bTemp;
		BigDecimal bSqrt;
		for (int i = 1; i < 100; i++) {
			bSqrt = BigDecimal.valueOf(i);
			bSqrt = NumHelp.sqrt(bSqrt, 120);
			System.out.println(bSqrt);
			bTemp = new BigInteger(bSqrt.toString().substring(2, 3));
			System.out.println(bTemp);
		}
	}
	
}
