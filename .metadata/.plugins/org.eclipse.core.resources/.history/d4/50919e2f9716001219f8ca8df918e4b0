package problem;

import extras.Sums;

public class Problem78 {
	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Sums s = new Sums(400);
		for (int i = 1; i < 400; i++) {
			if (s.allSumsUnder(i, i) % 100 == 0) {
				System.out.println(i + ": " + s.allSumsUnder(i, i));
			}
		}
	}
}
